type Image {
  id: ID!
  filename: String!
  faceDetected: FaceDetected
}
type FaceDetected {
  faceDetected: Boolean!
}
type Query {
  images: [Image]
  imagesUnprocessed: [Image]
}
type Mutation {
  addImage(filename: String!): Image
  addFaceDetected(imageId: ID!, faceDetected: Boolean!): FaceDetected
}
