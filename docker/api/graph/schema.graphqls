type Image {
  id: Int!
  filename: String!
  Detection: Detection
}
type Detection {
  person: Float!
}
type Query {
  images: [Image]
  image(imageId: Int!): Image
  takeImage: String!
}
type Mutation {
  addImage(filename: String!): Image
  addDetection(imageId: Int!, person: Float!): Detection
}
