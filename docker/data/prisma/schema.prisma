generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Image {
  id          Int           @id @default(autoincrement())
  filename    String
  faceDetected FaceDetected?
}

model FaceDetected {
  id           Int     @id @default(autoincrement())
  faceDetected Boolean
  image        Image   @relation(fields: [imageId], references: [id])
  imageId      Int     @unique
}
