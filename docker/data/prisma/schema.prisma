generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Image {
  filename String @id
  faceDetected   faceDetected?    @relation()
}

model faceDetected {
  filename  String @id
  faceDetected Boolean
  image    Image    @relation(fields: [filename], references: [filename])
}
